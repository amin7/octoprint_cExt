<div  class="tab-pane" >
	<label>File: <span data-bind="text: file_name()"></span><span data-bind="text: analysis_data()"></span></label>
    <label>grid_area: <span  data-bind="text:grid_area "></span>mm</label>
	<div class="w-100"></div> 
	<span id=id_cext_state></span>
	<label class="checkbox"><input type="checkbox"data-bind="checked: swap_xy" >swap_xy</label>    	
    <button class="btn btn-warning" type="button" style="width:48%" data-bind="enable: is_file_analysis, click: probe_area">Probe Area</button>
    <button class="btn btn-primary" type="button" style="width:48%" data-bind="enable: is_engrave_avaliable, click: function(){send_single_cmd('engrave')}" >Engrave</button>
    <button class="btn btn-secondary btn-block" data-bind="click: function(){send_single_cmd('probe_area_stop')}">Probe Cancel/Stop</button>

	<hr>        
    <button class="btn btn-dark btn-block" type="button" data-bind="enable: isOperational(),click: function() { OctoPrint.control.sendGcode(cmd_SetPosition000); }">Set (0)</button>
    <button class="btn btn-warning btn-block" data-bind="click: function() { z_hop(z_travel());absolute_move_xy(0,0); probe(z_travel(),speed_probe_fast());}" >Goto XY(0)</button>
	<button class="btn btn-primary btn-block" data-bind="enable: isOperational(), click: function() { OctoPrint.control.sendGcode(cmd_DisableSteppers); }" >Motors Off</button>
    <hr>
    <button class="btn btn-warning col-sm" type="button" data-bind="click: function() {z_hop(z_travel());}">z_hop</button>
    <input type="number" id="id_z_tool_change" min="1" data-bind="value: z_tool_change()">
    <button class="btn btn-info col-sm" type="button" data-bind="click: function() {z_hop($('#id_z_tool_change').val());}">ToolChange_z_hop</button>
    <br>
    <div style="display: flex;  flex-direction: column;align-items:center" >
        <button class="btn btn-info" data-bind="enable: isOperational() && is_file_analysis, click: function() {relative_move_xy(0, file_selected_depth_grid());}">GotoY +depth</button>        
        <div>
            <button class="btn btn-info" data-bind="enable: isOperational() && is_file_analysis, click: function() {relative_move_xy(-file_selected_width_grid(),0);}">GotoX -width</button>
            <button class="btn btn-info" data-bind="enable: isOperational() && is_file_analysis, click: function() {relative_move_xy( file_selected_width_grid(),0);}">GotoX +width</button>
        </div>
        <button class="btn btn-info" data-bind="enable: isOperational() && is_file_analysis, click: function() {relative_move_xy(0,-file_selected_depth_grid());}">GotoY -depth</button>
    </div>
    <br>
    <button class="btn" data-bind="click:  function() { OctoPrint.control.sendGcode(cmd_SetPositionZ0); }">set Z(0)</button>
    <button class="btn btn-warning" type="button" data-bind="click: function() {probe_threshold(z_travel(),speed_probe_fast());}">Probe</button>
    <button class="btn col-sm" data-bind="click: function() {probe_threshold(z_travel()/2,speed_probe_fine());}">Probe Fine</button>
    <button class="btn btn-info col-sm" type="button" data-bind="click: function() {probe_threshold($('#id_z_tool_change').val(),speed_probe_fast());}">ToolChange_Probe</button>
    <span data-bind="text:probing_state "></span>
    <hr>
    <button class="btn btn-warning btn-block" >Delta Steper</button>
    <input type="number" >
    <button class="btn btn-warning" >Apply Delta</button>
</div>

<div class="tab-pane" >
    <div>
    	<span >File: <span data-bind="text: file_name()"></span></span>
        <button type="button" class="btn btn-default pull-right btn-fluid" title="refresh" data-bind="click:function(){refresh()}">
            <span class="glyphicon glyphicon-icon-refresh" aria-hidden="true"></span>R
        </button>
        <label class="checkbox checkbox-fluid pull-right" >
            <input type="checkbox" data-bind="checked: log_scroll"> log_scroll
        </label>
    </div>
    <textarea id="id_cnc_extention_log" rows="7" style="width:100%;resize:vertical;" readonly ></textarea>
    <div class="row-fluid">
        <div class="span4"> 
            <button class="btn btn-danger btn-block" data-bind="click:function(){send_single_cmd('plane_reset')}">plane_reset</button>
        </div>
        <div class="span4">
            <button class="btn btn-secondary btn-block" data-bind="enable: is_file_analysis(), click:function(){send_single_cmd('swap_xy')}">swap_xy</button>
        </div>
        <div class="span4">   
            <button class="btn btn-secondary btn-block" data-bind="enable: is_file_analysis(), click:function(){send_single_cmd('apply_xy_offset')}">apply_xy_offset</button>
        </div> 
    </div>
    <div class="row-fluid">
        <div class="span4"> 
            <button class="btn btn-warning btn-block"data-bind="enable: is_file_analysis(), click: probe_area">Probe Area</button>
        </div>
        <div class="span4"> 
            <button class="btn btn-secondary btn-block" data-bind="click: function(){send_single_cmd('probe_area_stop')}">Probe Stop</button>
        </div>
        <div class="span4"> 
            <button class="btn btn-warning btn-secondary btn-block" data-bind="click: function(){send_single_cmd('probe_area_skip')}">Probe Area Skip</button>
        </div>
        <button class="btn btn-primary btn-block" data-bind="enable: is_engrave_avaliable(), click: function(){send_single_cmd('engrave')}" >Engrave</button>
    </div>
	<hr>        
    <button class="btn btn-dark btn-block" type="button" data-bind="enable: isOperational(),click: function() { OctoPrint.control.sendGcode(cmd_SetPosition000); }">Set (0)</button>
	<button class="btn btn-primary btn-block" data-bind="enable: isOperational(), click: function() { OctoPrint.control.sendGcode(cmd_DisableSteppers); }" >Motors Off</button>
    <hr>
    <div class="row-fluid">
        <div class="btn-group btn-group-vertical span2">
            <button id="cnc_extention_Z_m_hi" class="btn btn-warning btn-block" data-bind="enable: isOperational(),click: function() {probe_threshold(settingsViewModel.settings.plugins.cnc_extention.preset_z_hi(),settingsViewModel.settings.plugins.cnc_extention.speed_probe());}">Z-</button> 
            <button id="cnc_extention_Z_m_mid" class="btn btn-warning btn-block" data-bind="enable: isOperational(),click: function() {probe_threshold(settingsViewModel.settings.plugins.cnc_extention.preset_z_mid(),settingsViewModel.settings.plugins.cnc_extention.speed_probe());}">Z-</button> 
            <button id="cnc_extention_Z_m_min" class="btn btn-warning btn-block" data-bind="enable: isOperational(),click: function() {probe_threshold(settingsViewModel.settings.plugins.cnc_extention.preset_z_min(),settingsViewModel.settings.plugins.cnc_extention.speed_probe());}">Z-</button> 
        </div>
        <div class="span8">
            <div class="span8 offset2"> 
                <button id="cnc_extention_Y_p_depth" class="btn btn-info btn-block" data-bind="enable: isOperational() && is_file_analysis(), click: function() {relative_move_xy(0, file_selected_depth_grid());} ">Y +depth</button> 
            </div>
            <div class="row-fluid">
                <div class="span4">
                    <button id="cnc_extention_X_m_width" class="btn btn-info btn-block" data-bind="enable: isOperational() && is_file_analysis(), click: function() {relative_move_xy(-file_selected_width_grid(),0);}">X -width</button>
                </div>
                <div class="span4">
                    <button class="btn btn-warning btn-block" data-bind="enable: isOperational(), click: function() { z_hop(z_travel());absolute_move_xy(0,0); probe(z_travel(),speed_probe());}" >Goto XY(0)</button>
                </div>
                <div class="span4">
                    <button id="cnc_extention_X_p_width" class="btn btn-info btn-block" data-bind="enable: isOperational() && is_file_analysis(), click: function() {relative_move_xy( file_selected_width_grid(),0);}">X +width</button>
                </div>
            </div>
            <div class="span8 offset2">
                <button id="cnc_extention_Y_m_depth" class="btn btn-info btn-block" data-bind="enable: isOperational() && is_file_analysis(), click: function() {relative_move_xy(0,-file_selected_depth_grid());}">Y -depth</button>
            </div>

        </div>
        <div class="btn-group btn-group-vertical span2">
            <button id="cnc_extention_Z_p_hi" class="btn btn-info btn-block" data-bind="enable: isOperational(),click: function() {z_hop(settingsViewModel.settings.plugins.cnc_extention.preset_z_hi());}">Z+</button> 
            <button id="cnc_extention_Z_p_mid" class="btn btn-info btn-block" data-bind="enable: isOperational(),click: function() {z_hop(settingsViewModel.settings.plugins.cnc_extention.preset_z_mid());}">Z+</button> 
            <button id="cnc_extention_Z_p_min" class="btn btn-info btn-block" data-bind="enable: isOperational(),click: function() {z_hop(settingsViewModel.settings.plugins.cnc_extention.preset_z_min());}">Z+</button> 
        </div>
    </div>
    <label class="span12"> Probe state: <span data-bind="text:probing_state "></span></label>
    <div class="row-fluid">
        <div class="span5">
            <button class="btn btn-block" data-bind="enable: isOperational(), click: function() { OctoPrint.control.sendGcode(cmd_SetPositionZ0); }">set Z(0)</button>
        </div>
        <div class="span3">
            <div class="input-append ">
                <input id="id_cnc_extention_steper_ajust" class="input-mini text-right" type="number" step="0.1">
                <div class="add-on">
                    <span class="input-group-text"> mm</span>
                </div>
            </div>
        </div>
        <div class="span4">
            <button class="btn btn-warning btn-fluid" data-bind="enable: isOperational(),click: function() {steper_ajust();}">Ajust Stepper</button>
        </div>
    </div>
</div>

<div class="tab-pane" >
    <div>
    	<span >File: <span data-bind="text: file_name()"></span></span>
        <button type="button" class="btn btn-default btn-small pull-right btn-fluid" title="refresh" data-bind="click:function(){refresh()}">
            <span class="glyphicon glyphicon-icon-refresh" aria-hidden="true"></span>R
        </button>
        <label class="checkbox checkbox-fluid pull-right" >
            <input type="checkbox" data-bind="checked: log_scroll"> log_scroll
        </label>
    </div>
    <textarea id="id_cnc_extention_log" rows="7" style="width:100%;resize:vertical;" readonly ></textarea>
    <div class="row-fluid">
        <div class="span4"> 
            <button class="btn btn-danger btn-block" data-bind="click:function(){send_single_cmd('plane_reset')}">plane_reset</button>
        </div>
        <div class="span4">
            <button class="btn btn-secondary btn-block" data-bind="enable: is_file_analysis(), click:function(){send_single_cmd('swap_xy')}">swap_xy</button>
        </div>
        <div class="span4">
            <button class="btn btn-secondary btn-block" data-bind="enable: is_file_analysis(), click:function(){send_single_cmd('apply_xy_offset')}">apply_xy_offset</button>
        </div> 
    </div>
    <div class="row-fluid">
        <div class="span4"> 
            <button class="btn btn-warning btn-block"data-bind="enable: is_file_analysis(), click: probe_area">Probe Area</button>
        </div>
        <div class="span4"> 
            <button class="btn btn-secondary btn-block" data-bind="click: function(){send_single_cmd('probe_area_stop')}">Probe Stop</button>
        </div>
        <div class="span4"> 
            <button class="btn btn-warning btn-secondary btn-block" data-bind="click: function(){send_single_cmd('probe_area_skip')}">Probe Area Skip</button>
        </div>
        <button class="btn btn-primary btn-block" data-bind="enable: is_engrave_avaliable(), click: function(){send_single_cmd('engrave')}" >Engrave</button>
    </div>
	<hr>        
    <button class="btn btn-dark btn-block" type="button" data-bind="enable: isOperational(),click: function() { OctoPrint.control.sendGcode(cmd_SetPosition000); }">Set (0)</button>
    <button class="btn btn-block" data-bind="enable: isOperational(), click: function() { z_hop(z_travel());absolute_move_xy(0,0); probe(z_travel(),settingsViewModel.settings.plugins.cnc_extention.speed_probe());}" >Goto XY(0)</button>
	<button class="btn btn-primary btn-block" data-bind="enable: isOperational(), click: function() { OctoPrint.control.sendGcode(cmd_DisableSteppers); }" >Motors Off</button>
    <hr>
    <div class="row-fluid">
        <div class="span9">
            <div class="span8 offset2"> 
                <button id="cnc_extention_Y_step_plus" class="btn btn-info btn-block" data-bind="enable: isOperational(), click: function() {relative_move_xy(0, file_selected_depth());} ">Y+</button> 
            </div>
            <div class="row-fluid">
                <div class="span4">
                    <button id="cnc_extention_X_step_minus" class="btn btn-info btn-block" data-bind="enable: isOperational(), click: function() {relative_move_xy(-file_selected_width(),0);}">X-</button>
                </div>
                <div class="span4">
                    <button class="btn btn-block" data-bind="enable: isOperational(), click: function() { z_hop(z_travel());absolute_move_xy(0,0); probe(z_travel(),settingsViewModel.settings.plugins.cnc_extention.speed_probe());}" >Set XY(0)</button>
                </div>
                <div class="span4">
                    <button id="cnc_extention_X_step_plus" class="btn btn-info btn-block" data-bind="enable: isOperational() , click: function() {relative_move_xy( file_selected_width(),0);}">X+</button>
                </div>
            </div>
            <div class="span8 offset2">
                <button id="cnc_extention_Y_step_minus" class="btn btn-info btn-block" data-bind="enable: isOperational(), click: function() {relative_move_xy(0,-file_selected_depth());}">Y-</button>
            </div>

        </div>
        <div class="btn-group btn-group-vertical span3">
            <button id="cnc_extention_Z_step_plus" class="btn btn-info btn-block" data-bind="enable: isOperational(),click: function() {z_hop(move_step_z());}">Z+</button> 
            <button class="btn btn-block" data-bind="enable: isOperational(), click: function() { OctoPrint.control.sendGcode(cmd_SetPositionZ0); }">set Z(0)</button>
            <button id="cnc_extention_Z_step_minus" class="btn btn-info btn-block" data-bind="enable: isOperational(),click: function() {probe_threshold(move_step_z(),settingsViewModel.settings.plugins.cnc_extention.speed_probe());}">Z-</button> 
        </div>
    </div>
    <br>
    <div class="row-fluid">
        <div class="btn-group btn-group span12">
            <button class="btn span2" data-bind="enable: isOperational(),click: function() {set_move_step_xyz(0.1);}">0.1</button> 
            <button class="btn span2" data-bind="enable: isOperational(),click: function() {set_move_step_xyz(1);}">1</button> 
            <button class="btn span2" data-bind="enable: isOperational(),click: function() {set_move_step_xyz(10);}">10</button> 
            <button class="btn span2" data-bind="enable: isOperational(),click: function() {set_move_step_xyz(100);}">100</button> 
            <button class="btn span2" data-bind="enable: isOperational()&& is_file_analysis(),click: function() {move_step_x(file_selected_width());move_step_y(file_selected_depth());}">from plane</button> 
        </div>
    </div>

    <label class="span12"> Probe state: <span data-bind="text:probing_state "></span></label>
    <div class="row-fluid">
        <div class="span5">
            <button class="btn btn-block" data-bind="enable: isOperational(), click: function() { OctoPrint.control.sendGcode(cmd_SetPositionZ0); }">set Z(0)</button>
        </div>
        <div class="span3">
            <div class="input-append ">
                <input id="id_cnc_extention_steper_ajust" class="input-mini text-right" type="number" step="0.1">
                <div class="add-on">
                    <span class="input-group-text"> mm</span>
                </div>
            </div>
        </div>
        <div class="span4">
            <button class="btn btn-warning btn-fluid" data-bind="enable: isOperational(),click: function() {steper_ajust();}">Ajust Stepper</button>
        </div>
    </div>
</div>

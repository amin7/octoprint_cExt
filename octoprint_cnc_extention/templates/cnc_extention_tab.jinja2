<div class="tab-pane" >
    <div>
    	<span >File: <span data-bind="text: file_name()"></span></span>
        <button type="button" class="btn btn-default btn-small pull-right btn-fluid" title="refresh" data-bind="click:function(){refresh()}">
            <span class="glyphicon glyphicon-icon-refresh" aria-hidden="true"></span>R
        </button>
        <label class="checkbox checkbox-fluid pull-right" >
            <input type="checkbox" data-bind="checked: log_scroll"> log_scroll
        </label>
    </div>
    <textarea id="id_cnc_extention_log" rows="7" style="width:100%;resize:vertical;" readonly ></textarea>
    <div class="row-fluid">
        <div class="span4"> 
            <button class="btn btn-danger btn-block" data-bind="click:function(){send_single_cmd('plane_reset')}">plane_reset</button>
        </div>
        <div class="span4">
            <button class="btn btn-secondary btn-block" data-bind="enable: is_file_analysis(), click:function(){send_single_cmd('swap_xy')}">swap_xy</button>
        </div>
        <div class="span4">
            <button class="btn btn-secondary btn-block" data-bind="enable: is_file_analysis(), click:function(){send_single_cmd('apply_xy_offset')}">apply_xy_offset</button>
        </div> 
    </div>
    <br>
    <div class="row-fluid"> 
        <button class="btn span6" type="button" data-bind="enable: isOperational(),click: function() { OctoPrint.control.sendGcode(cmd_SetPositionX0Y0);send_single_cmd('probe_area_stop');}">Set XY(0)</button>
        <button class="btn span6" data-bind="enable: isOperational(), click: function() { OctoPrint.control.sendGcode(cmd_SetPositionZ0);}">set Z(0)</button>
    </div>
    <br>
    <div class="row-fluid">
        <div class="span4"> 
            <button class="btn btn-warning btn-block"data-bind="enable: is_file_analysis(), click: probe_area">Probe Area</button>
        </div>
        <div class="span4"> 
            <button class="btn btn-secondary btn-block" data-bind="click: function(){send_single_cmd('probe_area_stop')}">Probe Stop</button>
        </div>
        <div class="span4"> 
            <button class="btn btn-warning btn-secondary btn-block" data-bind="click: function(){send_single_cmd('probe_area_skip')}">Probe Area Skip</button>
        </div>
    </div>
    <br>
        <button class="btn btn-primary btn-block" data-bind="enable: is_engrave_avaliable(), click: function(){send_single_cmd('engrave')}" >Engrave</button>
	<hr>
    <div class="row-fluid">
        <div class="span9">
            <div class="span8 offset2"> 
                <button id="cnc_extention_Y_step_plus" class="btn btn-info btn-block" data-bind="enable: isOperational(), click: function() {relative_move_xy(0, move_step_y());} ">Y+</button> 
            </div>
            <div class="row-fluid">
                <div class="span4">
                    <button id="cnc_extention_X_step_minus" class="btn btn-info btn-block" data-bind="enable: isOperational(), click: function() {relative_move_xy(-move_step_x(),0);}">X-</button>
                </div>
                <div class="span4">
                    <button class="btn btn-block" data-bind="enable: isOperational(), click: function() {absolute_move_xy(0,0);}" >goto XY(0)</button>
                </div>
                <div class="span4">
                    <button id="cnc_extention_X_step_plus" class="btn btn-info btn-block" data-bind="enable: isOperational() , click: function() {relative_move_xy( move_step_x(),0);}">X+</button>
                </div>
            </div>
            <div class="span8 offset2">
                <button id="cnc_extention_Y_step_minus" class="btn btn-info btn-block" data-bind="enable: isOperational(), click: function() {relative_move_xy(0,-move_step_y());}">Y-</button>
            </div>

        </div>
        <div class="btn-group btn-group-vertical span3">
            <button id="cnc_extention_Z_step_plus" class="btn btn-info btn-block" data-bind="enable: isOperational(),click: function() {relative_move_z(move_step_z());}">Z+</button> 
            <button class="btn btn-block" data-bind="enable: isOperational(),click: function() {absolute_move_z(0);}">goto Z(0)</button> 
            <button id="cnc_extention_Z_step_minus" class="btn btn-info btn-block" data-bind="enable: isOperational(),click: function() {probe_threshold(move_step_z());}">Z-</button> 
        </div>
    </div>
    <br>
    <div class="row-fluid">
        <div class="btn-group btn-group span8">
            <button class="btn span2" data-bind="enable: isOperational(),click: function() {set_move_step_xyz(0.1);}">0.1</button> 
            <button class="btn span2" data-bind="enable: isOperational(),click: function() {set_move_step_xyz(1);}">1</button> 
            <button class="btn span2" data-bind="enable: isOperational(),click: function() {set_move_step_xyz(10);}">10</button> 
            <button class="btn span2" data-bind="enable: isOperational(),click: function() {set_move_step_xyz(50);}">50</button> 
            <button class="btn span4" data-bind="enable: isOperational()&& is_file_analysis(),click: function() {move_step_x(file_selected_width());move_step_y(file_selected_depth());}">from plane</button> 
        </div>
        <label class="span4">Probe: <span data-bind="text:probing_state "></span></label>
    </div>
    <button class="btn btn-primary btn-block" data-bind="enable: isOperational(), click: function() { OctoPrint.control.sendGcode(cmd_DisableSteppers); }" >Motors Off</button>
</div>

<h4>CNC</h4>
<form class="form-horizontal">
	<div class="row-fluid">
		<div class="row-fluid">
			<div class="input-append span3">
				<label>feed_xy</label>
				<input class="input-mini text-right" type="number" data-bind="value: settings.plugins.cnc_extention.feed_xy"/>
				<div class="add-on">
					<span class="input-group-text">mm/s</span>
				</div>
			</div>
			<div class="input-append span3">
				<label>feed_z</label>
				<input class="input-mini text-right" type="number" step="0.1" data-bind="value: settings.plugins.cnc_extention.feed_z"/>
				<div class="add-on">
					<span class="input-group-text">mm/s</span>
				</div>
			</div>
			<div class="input-append span3">
				<label>Probing Speed </label>
				<input class="input-mini text-right" type="number" data-bind="value: settings.plugins.cnc_extention.speed_probe"/>
				<div class="add-on">
					<span class="input-group-text">mm/s</span>
				</div>
			</div>
		</div>
		<div class="row-fluid">
			<div class="input-append span3">
				<label>z_travel </label>
				<input class="input-mini text-right" type="number" data-bind="value: settings.plugins.cnc_extention.z_travel"/>
				<div class="add-on">
					<span class="input-group-text">mm</span>
				</div>
			</div>
			<div class="input-append span3">
				<label>z_threshold </label>
				<input class="input-mini text-right" type="number" step="0.1" data-bind="value: settings.plugins.cnc_extention.z_threshold"/>
				<div class="add-on">
					<span class="input-group-text">mm</span>
				</div>
			</div>
		</div>
		<h5>Probe Area</h5>
		<div class="row-fluid">
			<div class="input-append span3">
				<label>feed_xy</label>
				<input class="input-mini text-right" type="number" data-bind="value: settings.plugins.cnc_extention.probe_area_feed_xy"/>
				<div class="add-on">
					<span class="input-group-text">mm/s</span>
				</div>
			</div>
			<div class="input-append span3">
				<label>level_delta_z </label>
				<input class="input-mini text-right" type="number" step="0.1" data-bind="value: settings.plugins.cnc_extention.level_delta_z"/>
				<div class="add-on">
					<span class="input-group-text">mm</span>
				</div>
			</div>
			<div class="input-append span3">
				<label>grid_area</label>
				<input class="input-mini text-right" type="number" data-bind="value: settings.plugins.cnc_extention.grid_area"/>
				<div class="add-on">
					<span class="input-group-text">mm</span>
				</div>
			</div>
		</div>
	</div>
</form>